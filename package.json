{
  "version": "0.0.1",
  "scripts": {
    "start": "dotenvx run -- vitepress dev www",
    "build": "dotenvx run -- vitepress build www",
    "preview": "dotenvx run -- vitepress preview www",
    "favicon": "cd assets && npx sharp-cli -i ./quickreceitas.svg -o ../www/public/favicon.ico",
    "logo": "cd assets && npx sharp-cli -i ./quickreceitas.svg -o ../www/public/logo.png",
    "banner": "cd assets && npx sharp-cli -i ./quickreceitas.svg -o ../www/public/banner.png -q 40 resize 380 380"
  },
  "devDependencies": {
    "sharp": "^0.33.5",
    "sharp-cli": "^5.1.0",
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "@clerk/clerk-js": "^5.53.0",
    "@clerk/vue": "^1.2.1",
    "@dotenvx/dotenvx": "^1.36.0",
    "@supabase/supabase-js": "^2.48.1",
    "@types/vue": "^2.0.0"
  }
}
